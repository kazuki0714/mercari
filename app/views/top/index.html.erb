
  <h4>商品一覧</h4>
<div class="main posts-index">
  <% @items.each do |item| %>
  　<div class="item-container">
     <div class="item-image">
           <img src="<%= "/item_images/#{item.image}" %>">
     </div>
     <div class="item-title">
           <%= item.title %>
     </div>
     <div class="item-price">
           <%= item.price %>
     </div>
   </div>
  <% end %>
</div>
