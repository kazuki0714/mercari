投稿一覧

<div>
  <% @items.each do |item| %>
       <strong>商品名</strong>
        <p>
      　<%= item.title %>
        </p>
        <strong>商品画像</strong>
        <p>
        <img src="<%= item.image %>" width="100">
     　 </p>
      　<strong>価格</strong>
        <p>
        <%= item.price %>
        </p>
        <strong>在庫数</strong>
         <p>
        <%= item.stock %>
        </p>
   <% end %>

    <strong>購入者数</strong>
     <p>
    <%= @cart.quantity %>
    </p>

</div>
